<extension xmlns="http://ns.adobe.com/air/extension/4.0">
	<id>com.distriqt.Adverts</id>
	<versionNumber>5.3.161</versionNumber>
	<platforms>
		<platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>distriqt.extension.adverts.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.adverts.AdvertsExtension</initializer>
				<finalizer>com.distriqt.extension.adverts.AdvertsExtension</finalizer>
			</applicationDeployment>
		</platform>
 		<platform name="Android-x86">
			<applicationDeployment>
				<nativeLibrary>distriqt.extension.adverts.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.adverts.AdvertsExtension</initializer>
				<finalizer>com.distriqt.extension.adverts.AdvertsExtension</finalizer>
			</applicationDeployment>
		</platform> 
		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libAdverts.a</nativeLibrary>
				<initializer>AdvertsExtInitializer</initializer>
				<finalizer>AdvertsExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<!--
		<platform name="iPhone-x86">
			<applicationDeployment>
				<nativeLibrary>libAdverts.a</nativeLibrary>
				<initializer>AdvertsExtInitializer</initializer>
				<finalizer>AdvertsExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		-->
		<platform name="default"> 
			<applicationDeployment /> 
		</platform>
	</platforms>
</extension>
