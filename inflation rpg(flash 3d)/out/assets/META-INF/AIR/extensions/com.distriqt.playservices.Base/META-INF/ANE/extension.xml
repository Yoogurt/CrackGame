<extension xmlns="http://ns.adobe.com/air/extension/4.0">
	<id>com.distriqt.playservices.Base</id>
	<versionNumber>15.0.1</versionNumber>
	<platforms>
		<platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>distriqt.extension.playservices.base.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.playservices.base.BaseExtension</initializer>
				<finalizer>com.distriqt.extension.playservices.base.BaseExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="Android-x86">
			<applicationDeployment>
				<nativeLibrary>distriqt.extension.playservices.base.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.playservices.base.BaseExtension</initializer>
				<finalizer>com.distriqt.extension.playservices.base.BaseExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libPlayServicesBase.a</nativeLibrary>
				<initializer>PlayServicesBaseExtInitializer</initializer>
				<finalizer>PlayServicesBaseExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="iPhone-x86">
			<applicationDeployment>
				<nativeLibrary>libPlayServicesBase.a</nativeLibrary>
				<initializer>PlayServicesBaseExtInitializer</initializer>
				<finalizer>PlayServicesBaseExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="default"> 
			<applicationDeployment /> 
		</platform>
	</platforms>
</extension>
