<extension xmlns="http://ns.adobe.com/air/extension/4.0">
	<id>com.distriqt.androidsupport.V4</id>
	<versionNumber>27.0.2</versionNumber>
	<platforms>
		<platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>distriqt.extension.androidsupport.v4.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.androidsupport.v4.V4Extension</initializer>
				<finalizer>com.distriqt.extension.androidsupport.v4.V4Extension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="Android-x86">
			<applicationDeployment>
				<nativeLibrary>distriqt.extension.androidsupport.v4.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.androidsupport.v4.V4Extension</initializer>
				<finalizer>com.distriqt.extension.androidsupport.v4.V4Extension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libAndroidSupportV4.a</nativeLibrary>
				<initializer>AndroidSupportV4ExtInitializer</initializer>
				<finalizer>AndroidSupportV4ExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="iPhone-x86">
			<applicationDeployment>
				<nativeLibrary>libAndroidSupportV4.a</nativeLibrary>
				<initializer>AndroidSupportV4ExtInitializer</initializer>
				<finalizer>AndroidSupportV4ExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="default"> 
			<applicationDeployment /> 
		</platform>
	</platforms>
</extension>
