<extension xmlns="http://ns.adobe.com/air/extension/4.0">
	<id>com.distriqt.playservices.Auth</id>
	<versionNumber>15.0.1</versionNumber>
	<platforms>
		<platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>distriqt.extension.playservices.auth.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.playservices.auth.AuthExtension</initializer>
				<finalizer>com.distriqt.extension.playservices.auth.AuthExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="Android-x86">
			<applicationDeployment>
				<nativeLibrary>distriqt.extension.playservices.auth.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.playservices.auth.AuthExtension</initializer>
				<finalizer>com.distriqt.extension.playservices.auth.AuthExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libPlayServicesAuth.a</nativeLibrary>
				<initializer>PlayServicesAuthExtInitializer</initializer>
				<finalizer>PlayServicesAuthExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="iPhone-x86">
			<applicationDeployment>
				<nativeLibrary>libPlayServicesAuth.a</nativeLibrary>
				<initializer>PlayServicesAuthExtInitializer</initializer>
				<finalizer>PlayServicesAuthExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="default"> 
			<applicationDeployment /> 
		</platform>
	</platforms>
</extension>
